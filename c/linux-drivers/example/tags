KDIR	Makefile	/^KDIR := \/usr\/src\/linux-headers-$(shell uname -r)$/;"	m
KVER	Makefile	/^KVER ?= $(shell uname -r)  $/;"	m
LIGHT_MAJOR	led.c	34;"	d	file:
LIGHT_OFF	led.c	31;"	d	file:
LIGHT_ON	led.c	30;"	d	file:
PWD	Makefile	/^PWD  := $(shell pwd)  $/;"	m
cdev	led.c	/^	struct cdev cdev;		\/*字符设备cdev结构体*\/$/;"	m	struct:light_dev	typeref:struct:light_dev::cdev	file:
example_exit	example.c	/^module_exit(example_exit);$/;"	v
example_exit	example.c	/^static void example_exit(void)$/;"	f	file:
example_init	example.c	/^module_init(example_init);$/;"	v
example_init	example.c	/^static int example_init(void)$/;"	f	file:
light_cleanup	led.c	/^module_exit(light_cleanup);		\/*卸载函数*\/$/;"	v
light_cleanup	led.c	/^void light_cleanup(void)$/;"	f
light_dev	led.c	/^struct light_dev{$/;"	s	file:
light_devp	led.c	/^struct light_dev *light_devp;$/;"	v	typeref:struct:light_dev
light_fops	led.c	/^struct file_operations light_fops = {$/;"	v	typeref:struct:file_operations
light_gpio_init	led.c	/^void light_gpio_init(void)$/;"	f
light_init	led.c	/^int light_init(void)$/;"	f
light_init	led.c	/^module_init(light_init);		\/*驱动程序入口函数*\/$/;"	v
light_ioctl	led.c	/^long light_ioctl(struct file *filp, unsigned int cmd, unsigned long arg)$/;"	f
light_major	led.c	/^int light_major = LIGHT_MAJOR;$/;"	v
light_off	led.c	/^void light_off(void)$/;"	f
light_on	led.c	/^void light_on(void)$/;"	f
light_open	led.c	/^int light_open(struct inode *inode, struct file *filp)$/;"	f
light_read	led.c	/^static ssize_t light_read(struct file *filp, char __user *buf, size_t count, loff_t *f_pos)$/;"	f	file:
light_release	led.c	/^int light_release(struct inode *inode, struct file *filp)$/;"	f
light_setup_cdev	led.c	/^static void light_setup_cdev(struct light_dev *dev, int index)$/;"	f	file:
light_write	led.c	/^static ssize_t light_write(struct file *filp, const char __user *buf, size_t count, loff_t *f_pos)$/;"	f	file:
mymodule-objs	Makefile	/^mymodule-objs := example.o $/;"	m
obj-m	Makefile	/^obj-m := example.o $/;"	m
value	led.c	/^	unsigned char value;	\/*亮为1,灭为0,用户可读写*\/$/;"	m	struct:light_dev	file:
